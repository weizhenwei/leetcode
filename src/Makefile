OBJECTS = $(SRCS:%.cpp=%.o)
TARGETS = $(SRCS:%.cpp=%)  
all : $(TARGETS)
	for target in $(TARGETS); do \
		$(CXX) $(CXXFLAGS) -o $$target $$target.cpp; \
		done  

clean:
	-rm -f $(TARGETS) $(OBJECTS)  

.PHONY: all clean
